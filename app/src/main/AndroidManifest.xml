<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="testlab.example.kadeem.bdosanniv">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FirstActivity">
                <action android:name="android.intent.action.FirstActivity" />
        </activity>

        <activity android:name=".SecondActivity">
            <action android:name="android.intent.action.SecondActivity" />
        </activity>

        <activity android:name=".ThirdActivity">
            <action android:name="android.intent.action.ThirdActivity" />
        </activity>

        <activity android:name=".FourthActivity">
            <action android:name="android.intent.action.FourthActivity" />
        </activity>

        <activity android:name=".FifthActivity">
            <action android:name="android.intent.action.FifthActivity" />
        </activity>

        <activity android:name=".SixthActivity">
            <action android:name="android.intent.action.SixthActivity" />
        </activity>

    </application>

</manifest>